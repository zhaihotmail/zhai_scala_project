object test1 {
  def main(args: Array[String])
  {
    val x=100;
    val y=100;
    if(x!=y) 
    {
      println("x!=y")
    }
    else if (x>y)
    {
      println("x>y")
    }
    else
    {
      println("something else")
    }
    val z=x%y
    println (z)
    println(xplusy(9,89))
    testunit(9,89)
  }
  
  def xplusy(x : Int, y : Int) : Int=
  {
    val sum=x+y
    return sum
  }
  
  def testunit(w:Int,y:Int):Unit=
  {
    println("this is" +  w + "and" + y)
  }
  
  var greeting:String="heoll"
   greeting="aegag"
  println(greeting.toString())
  


}